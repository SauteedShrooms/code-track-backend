<p id="notice"><%= notice %></p>

<h1>Accessorries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Speed</th>
      <th>Efficency</th>
      <th>Power</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accessorries.each do |accessorry| %>
      <tr>
        <td><%= accessorry.name %></td>
        <td><%= accessorry.speed %></td>
        <td><%= accessorry.efficency %></td>
        <td><%= accessorry.power %></td>
        <td><%= link_to 'Show', accessorry %></td>
        <td><%= link_to 'Edit', edit_accessorry_path(accessorry) %></td>
        <td><%= link_to 'Destroy', accessorry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accessorry', new_accessorry_path %>
